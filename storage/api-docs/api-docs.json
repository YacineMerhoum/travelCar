    "components": {
        "schemas": {
            "Trip": {
                "title": "Trip",
                "required": [
                    "starting_point",
                    "ending_point",
                    "starting_at",
                    "available_places",
                    "price",
                    "user_id"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "starting_point": {
                        "type": "string",
                        "example": "New York"
                    },
                    "ending_point": {
                        "type": "string",
                        "example": "Los Angeles"
                    },
                    "starting_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-07-22T10:00:00Z"
                    },
                    "available_places": {
                        "type": "integer",
                        "example": 3
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 99.99
                    },
                    "user_id": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "User": {
                "title": "User",
                "required": [
                    "id",
                    "lastname",
                    "firstname",
                    "email",
                    "password",
                    "role"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "lastname": {
                        "type": "string",
                        "example": "Doe"
                    },
                    "firstname": {
                        "type": "string",
                        "example": "John"
                    },
                    "email": {
                        "type": "string",
                        "example": "john.doe@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "password123"
                    },
                    "role": {
                        "type": "string",
                        "example": "admin"
                    },
                    "avatar": {
                        "type": "string",
                        "example": "http://example.com/avatar.jpg"
                    }
                },
                "type": "object"
            }
        }
    },
